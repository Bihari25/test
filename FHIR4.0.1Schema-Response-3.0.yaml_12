openapi: 3.0.0
info:
  contact:
    name: IHR API Support
    email: ihr_api_support@uhg.com
  description: 'The IHR API allows registered applications to access data contained within a consolidated Individual Health Record for a UnitedHealth Care member with Medical, Dental and Pharmacy. The IHR API is developed using RESTful principles.'
  termsOfService: 'https://www.optumdeveloper.com/content/odv-optumdev/optum-developer/en/legal-terms/terms-of-use.html'
  title: individual-health-records
  version: '2.5.0'
servers:
  - url: '''POST /individual-health-records/v1.0/'''
x-serviceLevelObjectives:
  x-availability: '99.99'
  x-expectedMsgsPerDay: '10000'
  x-maxMsgsPerHour: '2000'
  x-responseTime: '500'
  x-throughput: '30'
  x-maxPayloadSize: 2MB

paths:

  /read:
    post:
      tags:
        - >-
          A name that can be used for this particular path and action, "/read"
          is reserved POST for GET pattern to eliminate PHI in GET parameters.
          This should be used for returning a single element as you would
          receive from a GET on a resource with an identifier
      summary: Create a Composition resource
      operationId: getIhr
      parameters: []
      responses:
        '200':
          description: Search Composition operation successful
          content:
            application/fhir+json:
              schema:
                $ref: '#/components/schemas/Bundle'
      requestBody:
        $ref: '#/components/requestBodies/Bundle'
      security:
        - oAuthSample: []

      description: A Brief Description of this resource Path and action

components:

  securitySchemes:
    oAuthSample:
      type: oauth2
      description: This API uses OAuth 2
      flows:
        clientCredentials:
          scopes: {}
          tokenUrl: >-
            https://cors-proxy.origin-elr-core.optum.com/token-server/auth/oauth/v2/token
  requestBodies:
    Bundle:
      content:
        application/fhir+json:
          schema:
            $ref: '#/components/schemas/Bundle'
      required: true
  schemas:
    Bundle:
      allOf:
        - $ref: '#/components/schemas/Resource'
        - type: object
          properties:
            id:
              type: string
              pattern: '[A-Za-z0-9\-\.]{1,64}'
              description: Unique id for the bundle. This may be any string value that does not contain spaces.
            type:
              type: string
              enum:
                - transaction
              description: Indicates the purpose of this bundle &ndash; how it is intended to be used.
            entry:
              type: array
              items:
                $ref: '#/components/schemas/Bundle_Entry'
      required:
        - id
        - type
        - entry
    Bundle_Entry:
      allOf:
        - $ref: '#/components/schemas/BackboneElement'
        - type: object
          properties:
            resource:
              $ref: '#/components/schemas/Resource'
    Goal:
      allOf:
        - $ref: '#/components/schemas/DomainResource'
        - type: object
          properties:
            meta:
              $ref: '#/components/schemas/Meta'
            contained:
              type: array
              description: 'Contained, inline Resources.'
              items:
                $ref: '#/components/schemas/Goal_Observation'
            Identifier:
              type: array
              description: 'A numeric or alphanumeric string that is associated with a single object or entity within a given system.'
              items:
                anyOf:
                  - $ref: '#/components/schemas/recordKey'
                  - $ref: '#/components/schemas/instanceId'
                  - $ref: '#/components/schemas/referenceIds'
                  - $ref: '#/components/schemas/relatedConditionInstanceIds'
              example:
                - type:
                    text: recordKey
                  value: 'lQTW7h-A=+l@2P{huiZS'
                - type:
                    text: instanceId
                  value: '36904128193011'
                - type:
                    text: referenceIds
                  value: '["\1234463535\"]'
                - type:
                    text: relatedConditionInstanceIds
                  value:
                    - 1234463535
                    - 12345678
            extension:
              type: array
              description: "All extensions in this list are defined in this specification and have a base URI. "
              items:
                anyOf:
                  - $ref: '#/components/schemas/sensitivityClasses'
                  - $ref: '#/components/schemas/dataSource'
                  - $ref: '#/components/schemas/clinicallyRelevantDate'
                  - $ref: '#/components/schemas/expectedAchievement'
                  - $ref: '#/components/schemas/presenceStateTerm'
                  - $ref: '#/components/schemas/goalTargetType'
              example:
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Sensitivity+Classes'
                  valueString: '["CommunicableDiseases"]'
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Data+Source+Method'
                  valueString: '["System Interface Data Acquisition Method"]'
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Clinically+Relevant+Date'
                  valueDateTime: '2019-06-19T21:09:29+00:00'
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Goal+Expected+Achievement'
                  valueString: We need to get the patient's diastolic BP in range
                - url: 'https://new-wiki.optum.com/pages/viewpage.action?spaceKey=IHRI&title=IHR+API+Presence+State'
                  valueCodeableConcept:
                    coding:
                      - display: Present
                    text: Present
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Target+Type'
                  valueCodeableConcept:
                    coding:
                      - display: Weight Control Goal
                    text: Weight Control Goal
            achievementStatus:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: 'An object containing attributes related to the goal status, also called a health status indicator.This is a calculated value, and determines whether the goals is Met or Not Met.'
              example:
                coding:
                  - display: Not Met
                text: Not Met
            category:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: 'The object that is the classification that the goal                              belongs to. '
              example:
                - coding:
                    - display: Nursing Outcome
                  text: Nursing Outcome
            description:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: A consumer-friendly textual description of the expected benefit to the patient
              example:
                coding:
                  - display: Body Mass Index (BMI) Monitoring Goal
                text: Body Mass Index (BMI) Monitoring Goal
            startDate:
              allOf:
                - $ref: '#/components/schemas/date'
            target:
              type: array
              items:
                allOf:
                  - $ref: '#/components/schemas/Goal_Target'
              description: Target outcome for the goal
              example:
                - detailString: '*Screening BMI Annual'
                  dueDate: '2020-12-12'
            outcomeReference:
              allOf:
                - $ref: '#/components/schemas/Reference'
              description: A consumer-friendly textual description of a condition being treated at the time of admission.
              example:
                reference: '#/components/schemas/Goal_Observation'
          required:
            - resourceType

    Goal_Observation:
      allOf:
        - properties:
            resourceType:
              type: string
              example: Observation
            id:
              type: string
              example: '1'
            effectiveDateTime:
              allOf:
                - $ref: '#/components/schemas/dateTime'
            valueString:
              type: string
              example: '1234567890'
    Goal_Target:
      type: object
      properties:
        detailString:
          type: string
          pattern: '[ \r\n\t\S]+'
          description: The textual description of the target of the goal.
          example: '*Screening BMI Annual'
        dueDate:
          allOf:
            - $ref: '#/components/schemas/date'
          example: '2020-12-12'
      required:
        - detailString
        - dueDate
    AllergyIntolerance:
      allOf:
        - $ref: '#/components/schemas/Resource'
        - type: object
          properties:
            resourceType:
              type: string
              description: An identifier assigned to the record.
              enum:
                - AllergyIntolerance
            identifier:
              type: array
              description: 'A numeric or alphanumeric string that is associated with a single object or entity within a given system.'
              items:
                anyOf:
                  - $ref: '#/components/schemas/recordKey'
                  - $ref: '#/components/schemas/instanceId'
                  - $ref: '#/components/schemas/referenceIds'
              example:
                - type:
                    text: recordKey
                  value: 'o*}Dci^Vj2DS9mggDvEzepfVVqzx{ZEsiUvmmn<+gbH%HEjP.xoJ0P2h8Y(h'
                - type:
                    text: instanceId
                  value: '19108821002069'
                - type:
                    text: referenceIds
                  value: '["2019-01-02T03:04:44Z-Rally-1sE"]'
            extension:
              type: array
              description: 'All extensions in this list are defined in this specification and have a base URI. '
              items:
                anyOf:
                  - $ref: '#/components/schemas/presenceStateTerm'
                  - $ref: '#/components/schemas/clinicallyRelevantDate'
                  - $ref: '#/components/schemas/sensitivityClasses'
                  - $ref: '#/components/schemas/dataSource'
              example:
                - url: 'https://new-wiki.optum.com/pages/viewpage.action?spaceKey=IHRI&title=IHR+API+Presence+State'
                  valueString: Present
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Clinically+Relevant+Date'
                  valueDateTime: '2019-06-19T21:09:29+00:00'
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Sensitivity+Classes'
                  valueString: '["Substance Abuse","Genetics"]'
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Data+Source+Method'
                  valueString: '["System Interface Data Acquisition Method"]'
            clinicalStatus:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: 'An object containing attributes related to a status.  Status value changes only when data is ingested into into the IHR, and no other internal IHR calculation is used to update this field.'
              example:
                coding:
                  - system: SNOMEDCT Qualifier Value Foreign Key
                    code: '55561003'
                    display: Active
                text: Active
            code:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: An object containing attributes related to the description an adverse reaction type.
              example:
                coding:
                  - system: ICD-10 Diagnosis
                    code: T45.1X5A
                    display: Adverse Effect of Antineoplastic and Immunosuppressive Drugs
                  - system: 'http://hl7.org/fhir/sid/icd-10-cm'
                    code: T45.1X5A
                text: Adverse reaction to immunosuppressive drugs
            onsetDateTime:
              allOf:
                - $ref: '#/components/schemas/dateTime'
              description: 'The date the person''s health condition was officially diagnosed, in the format ''yyyy-MM-ddTHH:mm:ssZ (UTC time).'
              example: '2019-06-09T00:00:00+00:00'
            recordedDate:
              allOf:
                - $ref: '#/components/schemas/dateTime'
              description: 'The date that the IHR first "knows" about the item, in the format ''yyyy-MM-ddTHH:mm:ssZ (UTC time). This date updates using algorithmic calculations based on data received about the item. This date is not a clinical date, but may be based on one.'
              example: '2019-06-09T00:00:00+00:00'
            note:
              type: array
              description: A free-text note added by the patient regarding the object. 500 character limit.
              items:
                $ref: '#/components/schemas/Annotation'
              example:
                - authorString: Jane Doe
                  text: The chemotherapy medication causes hives.
            reaction:
              type: array
              items:
                $ref: '#/components/schemas/AllergyIntolerance_Reaction'
    AllergyIntolerance_Reaction:
      allOf:
        - properties:
            substance:
              type: array
              items:
                $ref: '#/components/schemas/CodeableConcept'
              description: An object array of one or more allergens (causative agent) for the adverse reaxtion.
              example:
                - coding:
                    - system: SNOMEDCT Substance Foreign Key
                      code: '419933005'
                      display: Antineoplastic Agent
                  text: Chemotherapy Medication
            manifestation:
              description: An object array of one or more reactions to the causative agent (i.e. one or more manifestations).
              type: array
              items:
                $ref: '#/components/schemas/CodeableConcept'
              example:
                - coding:
                    - system: SNOMEDCT Clinical Finding Foreign Key
                      code: '39579001'
                      display: Anaphylaxis
                  text: Anaphylaxis
            extension:
              allOf:
                - $ref: '#/components/schemas/Extension'
              description: An object containing attributes related to the severity of the reaction.
              example:
                - url: reactionSeverity
                  valueCodeableConcept:
                    coding:
                      - system: HL7v2 Allergy Severity - Table 0128 Foreign Key
                        code: SV
                        display: Antineoplastic Agent
                    text: Chemotherapy Medication
    Condition:
      allOf:
        - $ref: '#/components/schemas/Resource'
        - type: object
          description: A numeric or alphanumeric string that is associated with a single object or entity within a given system.
          properties:
            resourceType:
              type: string
              example: Condition
            identifier:
              type: array
              items:
                anyOf:
                  - $ref: '#/components/schemas/recordKey'
                  - $ref: '#/components/schemas/instanceId'
                  - $ref: '#/components/schemas/relatedConditionInstanceIds'
                  - $ref: '#/components/schemas/relatedObservationInstanceIds'
                  - $ref: '#/components/schemas/relatedCareTeamInstanceIds'
                  - $ref: '#/components/schemas/sourceClaimIds'
                  - $ref: '#/components/schemas/referenceIds'
              description: A numeric or alphanumeric string that is associated with a single object or entity within a given system.
              required:
                - recordKey
                - instanceId
              example:
                - type:
                    text: recordKey
                  value: 'o*}Dci^Vj2DS9mggDvEzepfVVqzx{ZEsiUvmmn<+gbH%HEjP.xoJ0P2h8Y(h'
                - type:
                    text: instanceId
                  value: '19108821002069'
                - type:
                    text: relatedConditionInstanceIds
                    value: "[123456,789]"
                - type:
                    text: relatedObservationInstanceIds
                    value: "[123456,789]"
                - type:
                    text: relatedCareTeamInstanceIds
                    value: "[123456,789]"
                - type:
                    text: sourceClaimIds
                    value: "[\"123456\",\"789\"]"
                - type:
                    text: referenceIds
                  value: "[\"2019-01-02T03:04:44Z-Rally-1sE\"]"
            extension:
              type: array
              description: All extensions in this list are defined in this specification and have a base URI.
              items:
                anyOf:
                  - $ref: '#/components/schemas/sensitivityClasses'
                  - $ref: '#/components/schemas/dataSource'
                  - $ref: '#/components/schemas/clinicallyRelevantDate'
                  - $ref: '#/components/schemas/presenceStateTerm'
                  - $ref: '#/components/schemas/status'
                  - $ref: '#/components/schemas/clinicalCourse'
              example:
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Sensitivity+Classes'
                  valueString: '["Substance Abuse","Genetics"]'
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Data+Source+Method'
                  valueString: '["System Interface Data Acquisition Method"]'
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Clinically+Relevant+Date'
                  valueDateTime: '2019-06-19T21:09:29+00:00'
                - url: 'https://new-wiki.optum.com/pages/viewpage.action?spaceKey=IHRI&title=IHR+API+Presence+State'
                  valueString: Present
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Status'
                  valueCodeableConcept:
                    coding:
                      system: SNOMEDCT Qualifier Value Foreign Key
                      code: 55561003
                      display: Active
                    text: Active
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Clinical+Course'
                  valueString: Chronic
            code:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: An object containing attributes related to a condition. Conditions include current and past diseases/disorders.  Adverse reactions/allergies and signs/symptoms are excluded in this data class.
              example:
                coding:
                  - system: ICD-10 Diagnosis
                    code: J00
                    display: Common Cold
                  - system: 'http://hl7.org/fhir/sid/icd-10-cm'
                    code: J00
                text: Common Cold
            onsetDateTime:
              allOf:
                - $ref: '#/components/schemas/dateTime'
              description: The date the person's condition was first diagnosed in the format 'yyyy-MM-ddTHH:mm:ssZ (UTC time). This date is the clinical start date.
              example: '2019-06-09T00:00:00+00:00'
            recordedDate:
              allOf:
                - $ref: '#/components/schemas/dateTime'
              description: The date that the IHR first 'knows' about the item, in the format 'yyyy-MM-ddTHH:mm:ssZ (UTC time). This date updates using algorithmic calculations based on data received about the item. This date is not a clinical date, but may be based on one.
              example: '2019-06-09T00:00:00+00:00'
            note:
              type: array
              description: A free-text note added by the patient regarding the object. 500 character limit.
              items:
                $ref: '#/components/schemas/Annotation'
              example:
                authorString: Jane Doe
                text: The chemotherapy medication causes hives.
    CareTeam:
      allOf:
        - $ref: '#/components/schemas/DomainResource'
        - type: object
          properties:
            text:
              $ref: '#/components/schemas/Element'
            identifier:
              type: array
              items:
                anyOf:
                  - $ref: '#/components/schemas/recordKey'
                  - $ref: '#/components/schemas/referenceIds'
                  - $ref: '#/components/schemas/instanceId'
              description: A numeric or alphanumeric string that is associated with a single object or entity within a given system.
              example:
                identifier:
                  - system: recordKey
                    value: lQTW7h-A=+l@2J)gCQY=
                    assigner:
                      reference: "#1"
              required:
                - recordKey
                - instanceId
            extension:
              type: array
              items:
                anyOf:
                  - $ref: '#/components/schemas/healthcareservice'
                  - $ref: '#/components/schemas/PractitionerFlag'
                  - $ref: '#/components/schemas/selfRoleTerm'
              description: All extensions in this list are defined in this specification and have a base URI.
              example:
                extension:
                  - url: https://new-wiki.optum.com/pages/viewpage.action?spaceKey=IHRI&title=IHR+API+Presence+State
                    valueString: Present
            period:
              $ref: '#/components/schemas/Period'
            subject:
              $ref: '#/components/schemas/Reference'
            participant:
              type: array
              items:
                $ref: '#/components/schemas/CareTeam_Participant'

    CareTeam_Participant:
      allOf:
        - type: object
          properties:
            member:
              $ref: '#/components/schemas/Reference'
            period:
              $ref: '#/components/schemas/Period'
            role:
              type: array
              items:
                $ref: '#/components/schemas/CodeableConcept'
              description: A textual description of the role the person / entity plays in relation to an actor, such as Insurer, Health Proxy, Primary Care Physician, etc.
    Device:
      allOf:
        - $ref: '#/components/schemas/DomainResource'
        - type: object
          properties:
            identifier:
              type: array
              description: A numeric or alphanumeric string that is associated with a single object or entity within a given system.
              example:
                - type:
                    text: recordKey
                  value: o*}Dci^Vj2DS9mggDvEzepfVVqzx{ZEsiUvmmn<+gbH%HEjP.xoJ0P2h8Y(h
                - type:
                    text: instanceId
                  value: '19523524502060'
                - type:
                    text: referenceIds
                  value: '["2019-01-02T03:04:44Z-Rally-1sE"]'
              items:
                anyOf:
                  - $ref: '#/components/schemas/recordKey'
                  - $ref: '#/components/schemas/instanceId'
                  - $ref: '#/components/schemas/referenceIds'
                  - $ref: '#/components/schemas/relatedCareTeamInstanceIds'
            extension:
              type: array
              description: All extensions in this list are defined in this specification and have a base URI.
              items:
                anyOf:
                  - $ref: '#/components/schemas/medicalDeviceStatus'
                  - $ref: '#/components/schemas/presenceStateTerm'
                  - $ref: '#/components/schemas/medicalDeviceDate'
                  - $ref: '#/components/schemas/lastDispensedDate'
                  - $ref: '#/components/schemas/clinicallyRelevantDate'
                  - $ref: '#/components/schemas/sensitivityClasses'
                  - $ref: '#/components/schemas/dataSource'
              example:
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Status'
                  valueCodeableConcept:
                    coding:
                      - system: TEST-sourceVocabulary
                        code: TEST-sourceVocabularyCode
                        display: Active
                    text: Active
                - url: 'https://new-wiki.optum.com/pages/viewpage.action?spaceKey=IHRI&title=IHR+API+Presence+State'
                  valueString: Present
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Start+Date'
                  valueDateTime: '2019-06-10T00:00:00+00:00'
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Clinically+Relevant+Date'
                  valueDateTime: '2018-02-17T11:45:41+00:00'
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Device+Last+Dispense+Date'
                  valueDateTime: '2018-05-02T00:00:00+00:00'
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Data+Source+Method'
                  valueString:
                    - System Interface Data Acquisition Method
                - url: 'https://new-wiki.optum.com/display/IHRI/IHR+API+Sensitivity+Classes'
                  valueString:
                    - Substance Abuse
                    - Genetics
            type:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: A numeric or alphanumeric string that is associated with a single object or entity within a given system.
              example:
                coding:
                  - system: CPT4/HCPCS/CDT Vocabulary
                    code: E0603
                    display: 'Breast pump, electric (AC and/or DC)'
                  - system: 'http://www.ama-assn.org/go/cpt'
                    code: 99025
                text: Breast Pump
            owner:
              allOf:
                - $ref: '#/components/schemas/Identifier'
              description: 'An object containing attributes related to a health device supplier. The supplier, which can be a pharmacy, hospital, or medical professional.'
              example:
                identifier:
                  type:
                    coding:
                      - system: 'http://hl7.org/fhir/sid/us-npi'
                        code: NPI
                        display: National Provider Identifier
                  value: '1699718775'
                display: TEST-NAME-NPINUM
            note:
              type: array
              description: A free-text note added by the patient regarding the object. 500 character limit.
              items:
                $ref: '#/components/schemas/Annotation'
              example:
                - authorString: Jane Doe
                  text: The author of a free-text note that is added to a patient record.
    HealthcareService:
      allOf:
        - $ref: '#/components/schemas/DomainResource'
        - type: object
          properties:
            type:
              $ref: '#/components/schemas/CodeableConcept'
    Observation:
      allOf:
        - $ref: '#/components/schemas/DomainResource'
        - type: object
          properties:
            identifier:
              type: array
              items:
                anyOf:
                  - $ref: '#/components/schemas/recordKey'
                  - $ref: '#/components/schemas/instanceId'
                  - $ref: '#/components/schemas/referenceIds'
              description: A numeric or alphanumeric string that is associated with a single object or entity within a given system.
              example:
                - type:
                    text: recordKey
                  value: o*}Dci^Vj2DS9mggDvEzepfVVqzx{ZEsiUvmmn<+gbH%HEjP.xoJ0P2h8Y(h
                  assigner:
                    reference: "#1"
                - type:
                    text: instanceId
                  value: '123456789'
                  assigner:
                    reference: "#1"
              required:
                - recordKey
                - instanceId
            extension:
              type: array
              items:
                anyOf:
                  - $ref: '#/components/schemas/observationUnitOfMeasure'
                  - $ref: '#/components/schemas/dataSource'
                  - $ref: '#/components/schemas/sensitivityClasses'
              description: All extensions in this list are defined in this specification and have a base URI.
              example:
                extension:
                  - url: https://new-wiki.optum.com/pages/viewpage.action?spaceKey    =IHRI&title=IHR+API+Presence+State
                    valueString: Present
            code:
              type: array
              description:  A numeric or alphanumeric string that is associated with a single object or entity within a given system.
              items:
                $ref: '#/components/schemas/CodeableConcept'
              example:
                code:
                  coding:
                    - system: LOINC Observation Foreign Key
                      code: '14771-0'
                      display: Glucose Measurement - Fasting Blood (FBS)
                text: Fasting Blood Sugar
            effectiveDateTime:
              $ref: '#/components/schemas/dateTime'
            valueString:
              type: string
              example: '300'
            note:
              type: array
              items:
                allOf:
                  - $ref: '#/components/schemas/patientNote'
                  - $ref: '#/components/schemas/patientNoteAuthor'
              description: Further information about the statement.
              example: 'Further other information'
            referenceRange:
              type: array
              items:
                type: string
                description: Further information about the statement.
            component:
              allOf:
                - $ref: '#/components/schemas/observationValue'
                - $ref: '#/components/schemas/abnormalIndicator'
    Immunization:
      allOf:
        - $ref: '#/components/schemas/DomainResource'
        - type: object
          properties:
            identifier:
