              example:
                display: Inpatient Hospital
    Encounter_Hospitalization:
      allOf:
        - properties:
            admitSource:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: The textual description of the source of admission for the patient to the facility.
              example:
                coding:
                  - display: Self Referral from Home or Workplace - Nonhealthcare Facility Point of Origin
            dischargeDisposition:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: An object containing attributes to related to discharge disposition (the level of care) the patient was discharged to.
              example:
                coding:
                  - system: IP ADT Discharge Disposition
                    code: '01'
                    display: Discharged to Home or Self Care (Routine Discharge)
                text: Discharged to Home or Self Care (Routine Discharge)
    Encounter_Diagnosis:
      allOf:
        - type: object
          properties:
            condition:
              allOf:
                - $ref: '#/components/schemas/Encounter_Condition'
              description: A consumer-friendly textual description of a condition being treated at the time of admission.
              example:
                reference: '#1'
            use:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: A  textual description of a condition being treated at the time of admission.
              example:
                coding:
                  - system: 'http://hl7.org/implement/standards/fhir/valueset-diagnosis-role.html'
                    code: AD
                    display: Admitting Diagnosis
      required:
        - condition
    Encounter_Condition:
      allOf:
        - properties:
            resourceType:
              type: string
              example: Condition
            id:
              type: string
              example: '1'
            code:
              allOf:
                - $ref: '#/components/schemas/CodeableConcept'
              description: A  textual description of a condition being treated at the time of discharge.
              example:
                coding:
                  - system: SNOMEDCT Qualifier Value Foreign Key
                    code: '55561003'
                    display: Active
                text: Active
    SimpleQuantity:
      allOf:
        - $ref: '#/components/schemas/Quantity'
        - type: object
    Quantity:
      allOf:
        - type: object
          properties:
            value:
              type: number
              description: >-
                The value of the measured amount. The value includes an implicit
                precision in the presentation of the value.
            comparator:
              type: string
              enum:
                - <
                - <=
                - '>='
                - '>'
              description: >-
                How the value should be understood and represented &ndash;
                whether the actual value is greater or less than the stated
                value due to measurement issues; e.g. if the comparator is "<" ,
                then the real value is < stated value.
            unit:
              type: string
              pattern: '[ \r\n\t\S]+'
              description: A human&ndash;readable form of the unit.
            system:
              type: string
              pattern: \S*
              description: >-
                The identification of the system that provides the coded form of
                the unit.
            code:
              type: string
              pattern: '[^\s]+(\s[^\s]+)*'
              description: >-
                A computer processable form of the unit in some unit
                representation system.
    Duration:
      allOf:
